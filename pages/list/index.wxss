.container {
  padding: 10rpx;
}
.img_box {
  margin: 5rpx 0;
  padding: 20rpx;
}
.img_box image {
  display: block;
  width: 90%;
  margin: 0 auto;
}
.text_box {
  padding: 0 20rpx;
}
.name {
   font-size: 36rpx;
   padding: 15rpx 5rpx;
}
.des {
  color: #999;
  font-size: 32rpx;
  font-weight: 100;
  text-indent: 2em;
}
.item {
  padding: 8px;
  background-color: #fff;
  border-top: 1px solid #e4e4e4;
  color: #787878;
}

.item image {
  display: block;
  width: 129px;
  height: 114px;
}

.item-right {
  margin-left: 11px;
}

.title {
  margin-bottom: 25px;
}
.title text {
  font-size: 16px;
  color: #787878;
}

text {
  font-size: 15px;
  color: #444;
}
.c-1 {
  color: #09bb07;
}
.daodi{
  text-align: center;
  color: #ccc;
}


.line_y {
  width: 3px;
  height: 100%;
  height: 18px;
  display: inline-block;
  background-color: #09bb07;
}
view.text text {
  margin-left: 10px;
  line-height: 18px;
}
.top {
  padding-top: 20rpx;
  height: 60rpx;
